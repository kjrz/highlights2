<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Dashboard">

    <!--<Button-->
    <!--android:id="@+id/noiseButton"-->
    <!--style="@style/ButtonText"-->
    <!--android:background="@drawable/btn_black"-->
    <!--android:text="Button"/>-->

    <Button
        android:id="@+id/noiseButton"
        style="@style/ButtonText"
        android:background="@drawable/btn_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/galleryButton"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/galleryButton"
        android:text="@string/noise_button" />

    <Button
        android:id="@+id/cam2Button"
        style="@style/ButtonText"
        android:background="@drawable/btn_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cam2button"
        android:layout_above="@+id/galleryButton"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="39dp"
        android:layout_alignStart="@+id/galleryButton" />

    <Button
        android:id="@+id/galleryButton"
        style="@style/ButtonText"
        android:background="@drawable/btn_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/galleryButton"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
